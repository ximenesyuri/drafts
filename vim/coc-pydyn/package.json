{
  "name": "coc-pydyn",
  "version": "0.0.1",
  "description": "Runtime-based dynamic Python attribute completion for coc.nvim",
  "main": "index.js",
  "engines": {
    "coc": ">=0.0.80"
  },
  "keywords": [
    "coc.nvim",
    "python",
    "completion",
    "dynamic"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "contributes": {
    "configuration": {
      "properties": {
        "pydyn.pythonPath": {
          "type": "string",
          "default": "python3",
          "description": "Python interpreter (typically from venv)"
        }
      }
    }
  },
  "dependencies": {
    "coc.nvim": ">=0.0.80"
  }
}
